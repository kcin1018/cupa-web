<h4>Create a Team</h4>
<form method="post">
    <div class="row">
        <dt>Name:</dt>
        <dd>
            <input type="text" name="name" id="team-name"/>
        </dd>
    </div>
    <div class="row submit">
        <input class="button" id="add-team-submit" type="submit" value="Create Team" />
    </div>
    <div id="error-string"></div>
</form>
