<div id="page-links" class="<?php echo $this->escape($this->pageClass()); ?>">
    <ul>
        <?php foreach($this->links as $link): ?>
        <?php if($link->name == $this->page->name): ?>
        <li><?php echo $this->escape($link->title); ?></li>
        <?php else: ?>
        <li><a href="<?php echo $this->url(array('page' => $link->name), 'page_view'); ?>"><?php echo $this->escape($link->title); ?></a></li>
        <?php endif; ?>
        <?php endforeach; ?>
    </ul>
</div>
